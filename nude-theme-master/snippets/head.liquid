<meta content="text/html; charset=UTF-8" http-equiv="Content-Type">
<meta content="width=device-width,initial-scale = 1.0" name="viewport">
<meta http-equiv="X-UA-Compatible" content="IE=edge">

<title>{{ page_title }}{% if settings.title %} — {{ settings.title }}{% else %}{% unless page_title contains shop.name %} — {{ shop.name }}{% endunless %}{% endif %}{% if site.subtitle %} | {{ site.subtitle }}{% endif %}</title>
<meta name="description" content="{% if page_description %}{{ page_description }}{% elsif site.meta_description %}{{ site.meta_description }}{% endif %}">
<meta name="web_author" content="gb Studio / grantblakeman.com">

{% include 'fb_meta' %}

<link href="https://fonts.googleapis.com/css?family=Assistant:300,700" rel="stylesheet">
{{ 'nude.scss.css' | asset_url | stylesheet_tag }}

<link rel="icon" href="{{ 'favicon.ico' | asset_url }}">
<link rel="apple-touch-icon" href="{{ 'touch-icon-iphone.png' | asset_url }}">
<link rel="apple-touch-icon" sizes="76x76" href="{{ 'touch-icon-ipad.png' | asset_url }}">
<link rel="apple-touch-icon" sizes="120x120" href="{{ 'touch-icon-iphone-retina.png' | asset_url }}">
<link rel="apple-touch-icon" sizes="152x152" href="{{ 'touch-icon-ipad-retina.png' | asset_url }}">

{% comment %}
  Load required Shopify JS + jQuery
{% endcomment %}
{{ 'shopify_common.js'  | asset_url | script_tag }}
{{ 'option_selection.js' | shopify_asset_url | script_tag }}
{{ '//ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js' | script_tag }}

